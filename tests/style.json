{
    "preset": "laravel",
    "rules": {
        "@PHP83Migration": true,
        "concat_space": {
            "spacing": "one"
        },
        "blank_line_before_statement": {
            "statements": [
                "declare",
                "phpdoc",
                "continue",
                "return"
            ]
        }
    },

    "extraSorting": {
        "foo": "Foo",
        "bar": "Bar"
    }
}
